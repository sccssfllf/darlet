// Математика
var x = 100;
var y = 3;
var res = x / y; // Цілочисельне ділення (33)

print("Division 100 / 3:");
print(res);

// Вкладений IF
if (res > 10) {
    if (res < 50) {
        print("Nested IF works: 10 < 33 < 50");
    }
}

